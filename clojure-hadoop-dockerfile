from sequenceid/hadoop:2.7.1
curl https://raw.githubusercontent.com/technomancy/leiningen/stable/bin/lein > lein
chmod 755 lein
mv lein /usr/local/bin/
export LEIN_ROOT="root"
yum install git
git clone https://github.com/johnmarinelli/clojure-hadoop
cd clojure-hadoop
lein deps
java -cp examples.jar clojure_hadoop.examples.wordcount1 README.txt out1
